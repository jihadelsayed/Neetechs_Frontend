{
  "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
  "version": 1,
  "newProjectRoot": "projects",
  "projects": {
    "neetechs": {
      "i18n": {
        "sourceLocale": "en-US",
        "locales": {
          "ar": "src/locale/messages.ar.xlf",
          "es": "src/locale/messages.es.xlf",
          "pt": "src/locale/messages.pt.xlf",
          "de": "src/locale/messages.de.xlf",
          "ru": "src/locale/messages.ru.xlf",
          "sv": "src/locale/messages.sv.xlf",
          "zh": "src/locale/messages.zh.xlf",
          "fr": "src/locale/messages.fr.xlf",
          "en": "src/locale/messages.xlf"
        }
      },
      "projectType": "application",
      "schematics": {
        "@schematics/angular:component": {
          "style": "scss"
        }
      },
      "root": "",
      "sourceRoot": "src",
      "prefix": "app",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:application",
          "options": {
            "localize": [
              "ar",
              "es",
              "pt",
              "ar",
              "de",
              "ru",
              "sv",
              "zh",
              "fr",
              "en"
            ],
            "outputPath": "dist/neetechs",
            "index": "src/index.html",
            "browser": "src/main.ts", 
            "polyfills": [
              "zone.js",
              "@angular/localize/init"
            ],
            "tsConfig": "tsconfig.app.json",
            "inlineStyleLanguage": "scss",
            "assets": [
              {
                "glob": "**/*",
                "input": "public",
                "output": "/"
              },
              {
                "glob": "manifest.webmanifest",
                "input": "src",
                "output": "/"
              },
              {
                "glob": "**/*",
                "input": "src/assets",
                "output": "/"
              }
            ],
            "styles": [
              "src/styles.scss",
              "src/styles.font.scss",
              "src/styles.form.scss",
              "src/styles.toast.scss"
              

            ],
            "scripts": [],
            "server": "src/main.server.ts",
            "prerender": true,
            "ssr": {
              "entry": "server.ts"
            }
          },
          "configurations": {
            "es": {
              "localize": [
                "es"
              ]
            },
            "en": {
              "localize": [
                "en"
              ]
            },
            "pt": {
              "localize": [
                "pt"
              ]
            },
            "ar": {
              "localize": [
                "ar"
              ]
            },
            "de": {
              "localize": [
                "de"
              ]
            },
            "ru": {
              "localize": [
                "ru"
              ]
            },
            "sv": {
              "localize": [
                "sv"
              ]
            },
            "zh": {
              "localize": [
                "zh"
              ]
            },
            "fr": {
              "localize": [
                "fr"
              ]
            },
            "public": {
            },
            "production": {
               
                
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "1000kb",
                  "maximumError": "2mb"
                },
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "3kb",
                  "maximumError": "4kb"
                }
              ],
              "fileReplacements": [
                {
                  "replace": "src/environments/environment.ts",
                  "with": "src/environments/environment.prod.ts"
                }
              ],
              "outputHashing": "all"
            },
            "development": {
              "buildOptimizer": false,
              "optimization": false,
              "vendorChunk": true,
              "extractLicenses": false,
              "sourceMap": true,
              "namedChunks": true
            }
          },
          "defaultConfiguration": "production"
        },
        "serve": {
          "builder": "@angular-devkit/build-angular:dev-server",
          "configurations": {
            "production": {
              "buildTarget": "neetechs:build:production"
            },
            "development": {
              "buildTarget": "neetechs:build:development"
            },
            "ar": {
              "buildTarget": "neetechs:build:ar"
            },
            "en": {
              "buildTarget": "neetechs:build:en"
            },
            "es": {
              "buildTarget": "neetechs:build:es"
            },
            "pt": {
              "buildTarget": "neetechs:build:pt"
            },
            "de": {
              "buildTarget": "neetechs:build:de"
            },
            "ru": {
              "buildTarget": "neetechs:build:ru"
            },
            "sv": {
              "buildTarget": "neetechs:build:sv"
            },
            "zh": {
              "buildTarget": "neetechs:build:zh"
            },
            "fr": {
              "buildTarget": "neetechs:build:fr"
            }
          },
          "defaultConfiguration": "production"
        },
        "extract-i18n": {
          "builder": "@angular-devkit/build-angular:extract-i18n",
          "options": {
            "buildTarget": "neetechs:build"
          }
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "main": "src/test.ts",
            "tsConfig": "tsconfig.spec.json",
            "karmaConfig": "karma.conf.js",
            "inlineStyleLanguage": "scss",
            "assets": [
              {
                "glob": "**/*",
                "input": "public",
                "output": "/"
              },
              {
                "glob": "manifest.webmanifest",
                "input": "src",
                "output": "/"
              }
            ],
            "styles": [
              "src/styles.scss",
              "src/styles.font.scss",
              "src/styles.form.scss",
              "src/styles.toast.scss"
            ],
            "scripts": []
          }
        },
        "lint": {
          "builder": "@angular-eslint/builder:lint",
          "options": {
            "lintFilePatterns": [
              "src/**/*.ts",
              "src/**/*.html"
            ]
          }
        }
      }
    }
  },
  "cli": {
    "analytics": false
  }
}